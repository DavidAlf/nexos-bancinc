{
    "name": "Java 21 Dev Container",
    "build": {
        "dockerfile": "Dockerfile",
        "args": {
            "VARIANT": "21"
        }
    },
    "settings": {
        "java.home": "/usr/lib/jvm/java-21-openjdk",
        "java.configuration.runtimes": [
            {
                "name": "JavaSE-21",
                "path": "/usr/lib/jvm/java-21-openjdk"
            }
        ]
    },
    "extensions": [
        "vscjava.vscode-java-pack"
    ]
}
